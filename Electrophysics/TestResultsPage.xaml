<Page x:Class="Electrophysics.TestResultsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Electrophysics"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TestResultsPage" Loaded="Page_Loaded">

    <Grid>
        <StackPanel>
            <TextBlock TextWrapping="Wrap" FontSize="50" Name="ResultTextBlock" Width="800" 
                       HorizontalAlignment="Center" Foreground="White" TextAlignment="Center"/>
            <Button Style="{StaticResource RoundBlueButton}" Content="На главный экран" Name="HomeButton" 
                    Click="HomeButton_Click" Width="700" Height="100" FontSize="60"
                    HorizontalAlignment="Center" Margin="0 200 0 0"/>
        </StackPanel>
    </Grid>
</Page>
